<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title
    <%= csrf_meta_tags %>
    <%= render 'layouts/stylesheets' %>
    <%= stylesheet_link_tag "application" %>
    <%= javascript_include_tag "application" %>
  </head>
  <body>
    <div class ="container">
      <header>
        <%= render 'layouts/header' %>
      </header>
      <section class="round">
           <%= yield %>
      </section>
      <%= render 'layouts/footer' %>
    </div>
  </body>
</html>

